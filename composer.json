{
    "name": "jbreynaud/fsm-generator",
    "description": "A PHP library for generating Finite State Machines (FSM) based on formal automata theory",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jbreynaud"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "FSM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSM\\Tests\\": "tests/"
        }
    }
}
